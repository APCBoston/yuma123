   <xs:simpleType name="DefaultStyle">
      <xs:annotation>
         <xs:documentation>
            Selects the type of filtering behavior the agent will
            advertise in the 'with-defaults' capability.  The agent
            will use this default handling behavior if the 
            'with-defaults' parameter is not explicitly set.
         </xs:documentation>
      </xs:annotation>
      <xs:restriction base="xs:string">
         <xs:enumeration value="report-all">
            <xs:annotation>
               <xs:documentation>
                  All default data is always reported.
               </xs:documentation>
               <xs:appinfo>
                  <ncx:value>1</ncx:value>
               </xs:appinfo>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="trim">
            <xs:annotation>
               <xs:documentation>
                  Values are not reported if they match the default.
               </xs:documentation>
               <xs:appinfo>
                  <ncx:value>2</ncx:value>
               </xs:appinfo>
            </xs:annotation>
         </xs:enumeration>
         <xs:enumeration value="explicit">
            <xs:annotation>
               <xs:documentation>
                  Report values if they are explicitly set.
               </xs:documentation>
               <xs:appinfo>
                  <ncx:value>3</ncx:value>
               </xs:appinfo>
            </xs:annotation>
         </xs:enumeration>
      </xs:restriction>
   </xs:simpleType>

