                                       </xs:documentation>
                                    </xs:annotation>
                                 </xs:element>
                                 <xs:element
                                    name="__.edit-config.input.target.config-target.startup.A__"
                                    minOccurs="0" maxOccurs="unbounded"
                                    abstract="true"/>
                              </xs:sequence>
                              <xs:sequence>
                                 <xs:annotation>
                                    <xs:appinfo>
                                       <ncx:case-name>url</ncx:case-name>
                                    </xs:appinfo>
