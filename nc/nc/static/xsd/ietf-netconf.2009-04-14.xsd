                                 <xs:element name="candidate"
                                    minOccurs="0">
                                    <xs:annotation>
                                       <xs:documentation>
                                          Only available if 'candidate' capability supported.
                                       </xs:documentation>
                                    </xs:annotation>
                                 </xs:element>
                                 <xs:element
                                    name="__.validate.input.source.config-source.candidate.A__"
                                    minOccurs="0" maxOccurs="unbounded"
                                    abstract="true"/>
                              </xs:sequence>
                              <xs:sequence>
                                 <xs:annotation>
                                    <xs:appinfo>
                                       <ncx:case-name>running</ncx:case-name>
                                    </xs:appinfo>
