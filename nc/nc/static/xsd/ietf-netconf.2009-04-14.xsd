                              </xs:sequence>
                              <xs:sequence>
                                 <xs:annotation>
                                    <xs:appinfo>
                                       <ncx:case-name>url</ncx:case-name>
                                    </xs:appinfo>
                                 </xs:annotation>
                                 <xs:element name="url"
                                    type="ConfigURIType" minOccurs="0">
                                    <xs:annotation>
