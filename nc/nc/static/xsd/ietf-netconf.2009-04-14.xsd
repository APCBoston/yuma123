                                    </xs:annotation>
                                 </xs:element>
                                 <xs:element
                                    name="__.validate.input.source.config-source.url.A__"
                                    minOccurs="0" maxOccurs="unbounded"
                                    abstract="true"/>
                              </xs:sequence>
                              <xs:sequence>
                                 <xs:annotation>
                                    <xs:appinfo>
                                       <ncx:case-name>config</ncx:case-name>
                                    </xs:appinfo>
                                 </xs:annotation>
