                           </xs:sequence>
                        </xs:complexType>
                        <xs:key name="session_Key">
                           <xs:selector xpath="."/>
                           <xs:field xpath="sessionId"/>
                        </xs:key>
                     </xs:element>
                     <xs:element
                        name="__.ietf-netconf-state.sessions.A__"
                        minOccurs="0" maxOccurs="unbounded"
                        abstract="true"/>
                  </xs:sequence>
               </xs:complexType>
            </xs:element>
            <xs:element name="subscriptions">
               <xs:annotation>
                  <xs:documentation>
                     Contains information on the active subscriptions on the
                     NETCONF server.  Subscriptions which have ended are not
                     reported.
                  </xs:documentation>
               </xs:annotation>
               <xs:complexType>
                  <xs:sequence>
                     <xs:element name="subscription" minOccurs="0"
                        maxOccurs="unbounded">
                        <xs:annotation>
                           <xs:documentation>
                              Information about Netconf Notification Subscriptions.
                           </xs:documentation>
                           <xs:appinfo>
                              <ncx:ordered-by>system</ncx:ordered-by>
                           </xs:appinfo>
                        </xs:annotation>
                        <xs:complexType>
                           <xs:sequence>
                              <xs:element name="sessionId"
                                 type="SessionId">
                                 <xs:annotation>
                                    <xs:documentation>
                                       The session id associated with this subscription.
                                    </xs:documentation>
                                 </xs:annotation>
                              </xs:element>
                              <xs:element name="stream"
                                 type="xs:string" minOccurs="0">
                                 <xs:annotation>
                                    <xs:documentation>
                                       The stream associated with this subscription.
                                    </xs:documentation>
                                 </xs:annotation>
                              </xs:element>
                              <xs:element name="filter"
                                 type="xs:anyType" minOccurs="0">
                                 <xs:annotation>
                                    <xs:documentation>
                                       The filters associated with this subscription.
                                    </xs:documentation>
                                    <ncx:reference>
                                       <ncx:text>
                                          RFC 4741: NETCONF Configuration Protocol
                                       </ncx:text>
                                       <ncx:url>
                                          http://www.ietf.org/rfc/rfc4741.txt
                                       </ncx:url>
                                    </ncx:reference>
                                 </xs:annotation>
                              </xs:element>
                              <xs:element name="startTime"
                                 type="yang:date-and-time"
                                 minOccurs="0">
                                 <xs:annotation>
                                    <xs:documentation>
                                       The startTime parameter from the create-subscription
                                       invokation, if it was present.
                                    </xs:documentation>
                                 </xs:annotation>
                              </xs:element>
                              <xs:element name="stopTime"
                                 type="yang:date-and-time"
                                 minOccurs="0">
                                 <xs:annotation>
                                    <xs:documentation>
                                       The stopTime parameter from the create-subscription
                                       invokation, if it was present.
                                    </xs:documentation>
                                 </xs:annotation>
                              </xs:element>
                              <xs:element name="outNotifications"
                                 type="yang:zero-based-counter32"
                                 default="0" minOccurs="0">
                                 <xs:annotation>
                                    <xs:documentation>
                                       A count of event notifications sent along
                                       this connection since the subscription was
                                       created.
                                    </xs:documentation>
                                 </xs:annotation>
                              </xs:element>
                              <xs:element
                                 name="__.ietf-netconf-state.subscriptions.subscription.A__"
                                 minOccurs="0" maxOccurs="unbounded"
                                 abstract="true"/>
                           </xs:sequence>
                        </xs:complexType>
                        <xs:key name="subscription_Key">
                           <xs:selector xpath="."/>
                           <xs:field xpath="sessionId"/>
                        </xs:key>
                     </xs:element>
                     <xs:element
                        name="__.ietf-netconf-state.subscriptions.A__"
                        minOccurs="0" maxOccurs="unbounded"
                        abstract="true"/>
                  </xs:sequence>
               </xs:complexType>
            </xs:element>
            <xs:element name="statistics">
               <xs:complexType>
                  <xs:sequence>
                     <xs:element name="netconfStartTime"
                        type="yang:date-and-time" minOccurs="0">
                        <xs:annotation>
                           <xs:documentation>
                              Date and time at which the NETCONF server process was
                              started.  Allows for calculation of time interval for
                              reported metrics.
                           </xs:documentation>
                        </xs:annotation>
                     </xs:element>
                     <xs:element name="inSessions"
                        type="yang:zero-based-counter32" default="0"
                        minOccurs="0">
                        <xs:annotation>
                           <xs:documentation>
                              The total number of NETCONF sessions started towards the
                              NETCONF peer.
                              
                               inSessions - inBadHellos = 'number of correctly started
                              			     netconf sessions'
                           </xs:documentation>
                        </xs:annotation>
                     </xs:element>
                     <xs:element name="inXMLParseErrors"
                        type="yang:zero-based-counter32" default="0"
                        minOccurs="0">
                        <xs:annotation>
                           <xs:documentation>
                              The total number of messages that were unparsable and thus
                              ignored.  This covers both unparsable 'hello' and 'rpc'
                              messages.
                           </xs:documentation>
                        </xs:annotation>
                     </xs:element>
                     <xs:element name="inBadHellos"
                        type="yang:zero-based-counter32" default="0"
                        minOccurs="0">
                        <xs:annotation>
                           <xs:documentation>
                              The total number of sessions silently dropped because an
                              invalid 'hello' message was received.  This includes hello
                              messages with a 'session-id' attribute, bad namespace, and
                              bad capability declarations.
                           </xs:documentation>
                        </xs:annotation>
                     </xs:element>
                     <xs:element name="inRpcs"
                        type="yang:zero-based-counter32" default="0"
                        minOccurs="0">
                        <xs:annotation>
                           <xs:documentation>
                              The total number of rpc requests received.
                           </xs:documentation>
                        </xs:annotation>
                     </xs:element>
                     <xs:element name="inBadRpcs"
                        type="yang:zero-based-counter32" default="0"
                        minOccurs="0">
                        <xs:annotation>
                           <xs:documentation>
                              The total number of rpcs which were parsed correctly, but
                              couldn't be serviced because they contained
                              non-conformant XML,  e.g. missing a mandatory parameter.
                           </xs:documentation>
                        </xs:annotation>
                     </xs:element>
                     <xs:element name="inNotSupportedRpcs"
                        type="yang:zero-based-counter32" default="0"
                        minOccurs="0">
                        <xs:annotation>
                           <xs:documentation>
                              The total number of rpcs which were parsed correctly, but
                              couldn't be serviced because they were not supported by
                              the agent.
                           </xs:documentation>
                        </xs:annotation>
                     </xs:element>
                     <xs:element name="outRpcReplies"
                        type="yang:zero-based-counter32" default="0"
                        minOccurs="0">
                        <xs:annotation>
                           <xs:documentation>
                              The total number of 'rpc-reply' messages sent.
                           </xs:documentation>
                        </xs:annotation>
                     </xs:element>
                     <xs:element name="outRpcErrors"
                        type="yang:zero-based-counter32" default="0"
                        minOccurs="0">
                        <xs:annotation>
                           <xs:documentation>
                              The total number of 'rpc-reply' messages with 'rpc-error'
                               sent.
                           </xs:documentation>
                        </xs:annotation>
                     </xs:element>
                     <xs:element name="outNotifications"
                        type="yang:zero-based-counter32" default="0"
                        minOccurs="0">
                        <xs:annotation>
                           <xs:documentation>
                              The total number of 'notifications' messages sent.
                           </xs:documentation>
                        </xs:annotation>
                     </xs:element>
                     <xs:element
                        name="__.ietf-netconf-state.statistics.A__"
                        minOccurs="0" maxOccurs="unbounded"
                        abstract="true"/>
                  </xs:sequence>
               </xs:complexType>
            </xs:element>
            <xs:element name="__.ietf-netconf-state.A__" minOccurs="0"
               maxOccurs="unbounded" abstract="true"/>
         </xs:sequence>
      </xs:complexType>
   </xs:element>

   <xs:complexType name="get-schema_output_type__">
      <xs:complexContent>
         <xs:extension base="nc:dataInlineType">
            <xs:sequence>
               <xs:element name="data" type="xs:anyType" minOccurs="0">
                  <xs:annotation>
                     <xs:documentation>
                        Contains the schema content.
                     </xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="__.get-schema.A__" minOccurs="0"
                  maxOccurs="unbounded" abstract="true"/>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>

   <xs:element name="get-schema" substitutionGroup="nc:rpcOperation">
      <xs:annotation>
         <xs:appinfo>
            <ncx:rpc-output>
               get-schema_output_type__
            </ncx:rpc-output>
         </xs:appinfo>
      </xs:annotation>
      <xs:complexType>
         <xs:complexContent>
            <xs:extension base="nc:rpcOperationType">
               <xs:sequence>
                  <xs:element name="identifier" type="xs:string">
                     <xs:annotation>
                        <xs:appinfo>
                           <ncx:mandatory>true</ncx:mandatory>
                        </xs:appinfo>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="version" type="xs:string">
                     <xs:annotation>
                        <xs:appinfo>
                           <ncx:mandatory>true</ncx:mandatory>
                        </xs:appinfo>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="format" type="SchemaFormat">
                     <xs:annotation>
                        <xs:appinfo>
                           <ncx:mandatory>true</ncx:mandatory>
                        </xs:appinfo>
                     </xs:annotation>
                  </xs:element>
                  <xs:element name="__.get-schema.A__" minOccurs="0"
                     maxOccurs="unbounded" abstract="true"/>
               </xs:sequence>
            </xs:extension>
         </xs:complexContent>
      </xs:complexType>
   </xs:element>

</xs:schema>
