         <span class="yang_kw">description</span>
            <span class="yang_str">"Internal object for the nc:operation attribute."</span>;
         <a href="/modules/ncx/2009-04-10#abstract.339">ncx:abstract</a>;
         <a href="/modules/ncx/2009-04-10#hidden.378">ncx:hidden</a>;
      }

      <a name="rpc.573"></a><span class="yang_kw">container</span> <span class="yang_id">rpc</span> {
         <span class="yang_kw">config</span> <span class="yang_str">"true"</span>;
         <span class="yang_kw">description</span> <span class="yang_str">"Remote Procedure Call request message"</span>;
         <span class="yang_kw">reference</span> <span class="yang_str">"RFC 4741, section 4.1"</span>;
         <a href="/modules/ncx/2009-04-10#metadata.389">ncx:metadata</a> <span class="yang_str">"MessageId message-id"</span>;
