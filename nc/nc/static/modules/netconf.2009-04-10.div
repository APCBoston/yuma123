            <a name="augment.529"></a><span class="yang_kw">augment</span> <a href="#config-source.488">config-source</a> {
               <a name="url.530"></a><span class="yang_kw">leaf</span> <span class="yang_id">url</span> {
                  <span class="yang_kw">if-feature</span> <span class="yang_id"><a href="/modules/netconf/2009-04-10#url.82">url</a></span>;
                  <span class="yang_kw">type</span> <span class="yang_id"><a href="#ConfigURIType.243">ConfigURIType</a></span>;
                  <span class="yang_kw">description</span>
                     <span class="yang_str">"URL pointing to config data. Only available
                     if 'url' capability supported."</span>;
               }

               <a name="config.538"></a><span class="yang_kw">container</span> <span class="yang_id">config</span> {
                  <span class="yang_kw">description</span> <span class="yang_str">"Inline configuration data"</span>;
                  <a href="/modules/ncx/2009-04-10#root.425">ncx:root</a>;
