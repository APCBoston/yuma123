            }
         }
      }  <span class="yang_cmt">// grouping RpcOperationSourceType</span>

      <a name="RpcOperationTargetType.546"></a><span class="yang_kw">grouping</span> <span class="yang_id">RpcOperationTargetType</span> {
         <span class="yang_kw">description</span> <span class="yang_str">"NETCONF 'target' Parameter contents."</span>;

         <a name="uses.549"></a><span class="yang_kw">uses</span> <a href="#CommonConfigSourceType.485">CommonConfigSourceType</a> {
            <a name="augment.550"></a><span class="yang_kw">augment</span> <a href="#config-source.488">config-source</a> {
               <a name="url.551"></a><span class="yang_kw">leaf</span> <span class="yang_id">url</span> {
                  <span class="yang_kw">if-feature</span> <span class="yang_id"><a href="/modules/netconf/2009-04-10#url.82">url</a></span>;
                  <span class="yang_kw">type</span> <span class="yang_id"><a href="#ConfigURIType.243">ConfigURIType</a></span>;
                  <span class="yang_kw">description</span>
                     <span class="yang_str">"URL pointing to desired config data output. Only 
                     available if 'url' capability supported."</span>;
               }
            }
         }
      }  <span class="yang_cmt">// grouping RpcOperationTargetType</span>

      <a name="operation.563"></a><span class="yang_kw">leaf</span> <span class="yang_id">operation</span> {
