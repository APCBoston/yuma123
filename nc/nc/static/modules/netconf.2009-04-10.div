               <a name="url.514"></a><span class="yang_kw">leaf</span> <span class="yang_id">url</span> {
                  <span class="yang_kw">if-feature</span> <span class="yang_id"><a href="/modules/netconf/2009-04-10#url.82">url</a></span>;
                  <span class="yang_kw">type</span> <span class="yang_id"><a href="#ConfigURIType.243">ConfigURIType</a></span>;
                  <span class="yang_kw">description</span>
                     <span class="yang_str">"URL pointing to config data. Only available
                     if 'url' capability supported."</span>;
               }
            }
         }
      }  <span class="yang_cmt">// grouping GetConfigSourceType</span>

      <a name="RpcOperationSourceType.525"></a><span class="yang_kw">grouping</span> <span class="yang_id">RpcOperationSourceType</span> {
