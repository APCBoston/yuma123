      }  <span class="yang_cmt">// container rpc</span>

      <a name="rpc-reply.583"></a><span class="yang_kw">container</span> <span class="yang_id">rpc-reply</span> {
         <span class="yang_kw">config</span> <span class="yang_str">"true"</span>;
         <span class="yang_kw">description</span> <span class="yang_str">"Remote Procedure Call response message"</span>;
