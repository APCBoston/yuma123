         <span class="yang_str">"urn:ietf:params:xml:ns:yang:smiv2:ENTITY-STATE-MIB"</span>;

      <span class="yang_kw">prefix</span> <span class="yang_str">"entity-state-mib"</span>;

      <span class="yang_kw">import</span> <a href="/modules/ENTITY-MIB/2005-08-10">ENTITY-MIB</a> {
