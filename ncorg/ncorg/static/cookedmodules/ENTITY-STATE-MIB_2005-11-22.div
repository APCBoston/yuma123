               <a href="/modules/yang-smi/2008-03-20#oid.21">smi:oid</a> <span class="yang_str">"1.3.6.1.2.1.131.1.1.1.2"</span>;
            }
         }  <span class="yang_cmt">// container entStateOperDisabled-entStateAdmin</span>

         <a name="entStateOperDisabled-entStateAlarm.392"/><span class="yang_kw">container</span> <span class="yang_id">entStateOperDisabled-entStateAlarm</span> {

            <a name="entPhysicalIndex.393"/><span class="yang_kw">leaf</span> <span class="yang_id">entPhysicalIndex</span> {
