                              <smi:default  smi:default=""/>
                              <smi:oid  smi:oid="1.3.6.1.6.3.13.1.3.1.2"/>
                           </xs:appinfo>
                        </xs:annotation>
                        <xs:simpleType>
                           <xs:restriction base="xs:base64Binary">
                              <xs:maxLength value="16"/>
                           </xs:restriction>
