                  <xs:selector xpath="."/>
                  <xs:field xpath="diffServConfigId"/>
               </xs:key>
            </xs:element>
            <xs:element name="__.diffServConfigMIBObjects.A__"
               minOccurs="0" maxOccurs="unbounded" abstract="true"/>
