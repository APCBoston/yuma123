               <xs:annotation>
                  <xs:documentation>
                     State information about this physical entity.
                  </xs:documentation>
                  <xs:appinfo>
                     <ncx:ordered-by>system</ncx:ordered-by>
                     <smi:oid  smi:oid="1.3.6.1.2.1.131.1.1.1"/>
                  </xs:appinfo>
