# Makefile for Yuma etc directory
#  
#   etc directory

ETC_INSTALL = $(DESTDIR)/etc/yuma

all:

depend:

clean:

uninstall:
	rm -f $(ETC_INSTALL)

superclean: clean


ifdef SERVER
install:
	cp netconfd-sample.conf $(ETC_INSTALL)/
else
ifdef CLIENT
install:
	cp yangcli-sample.conf $(ETC_INSTALL)/
	cp yangdiff-sample.conf $(ETC_INSTALL)/
	cp yangdump-sample.conf $(ETC_INSTALL)/
else
ifdef SHLIBS
install:
else
ifdef DEVELOPER
install:
else
install:
	mkdir -p $(ETC_INSTALL)
	cp netconfd-sample.conf $(ETC_INSTALL)/
	cp yangcli-sample.conf $(ETC_INSTALL)/
	cp yangdiff-sample.conf $(ETC_INSTALL)/
	cp yangdump-sample.conf $(ETC_INSTALL)/
endif
endif
endif
endif

distclean: superclean uninstall

.PHONY: all clean depend superclean install uninstall distclean

# prevent the make program from choking on all the symbols
# that get generated from autogenerated make rules
.NOEXPORT:



