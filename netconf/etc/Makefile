# Makefile for Yuma etc directory
#  
#   etc directory

TOP_INSTALL = /usr/share/yuma

all:

depend:

clean:
	sudo rm $(TOP_INSTALL)/etc/netconfd-sample.conf
	sudo rm $(TOP_INSTALL)/etc/yangcli-sample.conf
	sudo rm $(TOP_INSTALL)/etc/yangdiff-sample.conf
	sudo rm $(TOP_INSTALL)/etc/yangdump-sample.conf

superclean: clean

install:
	sudo cp netconfd-sample.conf $(TOP_INSTALL)/etc/
	sudo cp yangcli-sample.conf $(TOP_INSTALL)/etc/
	sudo cp yangdiff-sample.conf $(TOP_INSTALL)/etc/
	sudo cp yangdump-sample.conf $(TOP_INSTALL)/etc/


.PHONY: all clean depend superclean install

# prevent the make program from choking on all the symbols
# that get generated from autogenerated make rules
.NOEXPORT:



