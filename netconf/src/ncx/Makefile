# Makefile for NETCONF project
#  
#   ncx directory

############### SOURCE PROFILE ##############################

SUBDIR_NM=ncx
SUBDIR_CPP=

############### TARGET PROFILE ##############################

TARGET=$(TBASE)/$(SUBDIR_NM)
BIN_INST=$(TBASE)/bin


######################### MAKE RULES ######################

all: ncxdummy ncxlib

#################### PLATFORM DEFINITIONS ############
include ../platform/platform.profile

################ DEPENDENCIES #########################
# depend rule must be included after the 'all' make rule

include ../platform/platform.profile.depend


test:


install:


#ncxlib: dependencies $(LBASE)/lib$(SUBDIR_NM).a

ncxlib: dependencies $(LBASE)/lib$(SUBDIR_NM).so


# this dummy rule keeps make from deleting the $(OBJS) as
# intermediate files
ncxdummy: dependencies $(OBJS)

clean:
	rm -f $(OBJS) $(LBASE)/lib$(SUBDIR_NM).a \
	$(LBASE)/lib$(SUBDIR_NM).so*

superclean:
	rm -f *~  $(DEPS) dependencies $(OBJS) \
	$(LBASE)/lib$(SUBDIR_NM).a \
	$(LBASE)/lib$(SUBDIR_NM).so*


# prevent the make program from choking on all the symbols
# that get generated from autogenerated make rules
.NOEXPORT:

include dependencies


