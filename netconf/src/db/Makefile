# Makefile for NETCONF project
#  
#   src/db directory

############### SOURCE PROFILE ##############################

SUBDIR_NM= db
SUBDIR_CPP=


############### TARGET PROFILE ##############################

TARGET=$(TBASE)/$(SUBDIR_NM)
BIN_INST=$(TBASE)/bin

####################### MAKE RULES #######################
all: dblib dbdummy

#################### PLATFORM DEFINITIONS ############
include ../platform/platform.profile

################ DEPENDENCIES #########################
# depend rule must be included after the 'all' make rule
include ../platform/platform.profile.depend

# test: dbtest

install:

dblib: dependencies $(LBASE)/lib$(SUBDIR_NM).a

dbdummy: dependencies $(OBJS)


clean:
	rm -f $(OBJS) $(LBASE)/lib$(SUBDIR_NM).a

superclean:
	rm -f *~  $(DEPS) dependencies $(OBJS) $(LBASE)/lib$(SUBDIR_NM).a


# this real test rule keeps make from deleting the $(OBJS) as
# intermediate files
#dbtest: $(TARGET)/dbtest.o $(OBJS) 
#	$(LINK) $(LFLAGS) $(TARGET)/dbtest.o $(OBJS) \
#	-o $(BIN_INST)/dbtest -lutil -L/usr/local/lib -lodbc


# prevent the make program from choking on all the symbols
# that get generated from autogenerated make rules
.NOEXPORT:

include dependencies
