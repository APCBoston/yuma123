# Makefile for NETCONF project
#  
#   src/base directory

############### SOURCE PROFILE ##############################

SUBDIR_NM=test
SUBDIR_CPP= 

####################### MAKE RULES #############################
all: readtxt s dummy

#################### PLATFORM DEFINITIONS ############
# MUST BE AFTER DEPS ARE DEFINED
include ../platform/platform.profile

################ DEPENDENCIES #########################
# depend rule must be included after the 'all' make rule

include ../platform/platform.profile.depend


# this real rule keeps make from deleting the $(OBJS) as
# intermediate files
readtxt: ./readtxt.o
	$(LINK) $(LFLAGS) ./readtxt.o -o ./readtxt -lxml2 -L/usr/lib

s: ./s.o
	$(LINK) $(LFLAGS) ./s.o -o ./s

clean:
	rm --force $(OBJS) ./readtxt ./s

superclean:
	rm --force *~ dependencies *.D $(OBJS) ./readtxt ./s


# prevent the make program from choking on all the symbols
# that get generated from autogenerated make rules
.NOEXPORT:


# dependency rules generated by gcc
# Do not edit by hand!! 
# Run 'make depend' to update the Makefile
# 
#_%_ DO NOT DELETE OR EDIT THIS LINE, make depend needs it!!!



