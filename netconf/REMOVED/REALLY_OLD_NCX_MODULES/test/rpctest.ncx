ncx-module rpctest {
  header {
    description "RPC TEST";
    version 1.0;
    owner ncx;
    application test;
  }
  imports {
    import xsd;
    import netconf { RpcDataReplyType };    
  }

  definitions {

    rpc rpc1 {
      description "RPC 1 status test";
      rpc-type exec;
      parms {
        parm status {
          description "integer status parm";
          type int;
        }
      }
      output RpcDataReplyType;
    }

    rpc rpc2 {
      description "RPC 2 status test";
      rpc-type exec;
      parms {
        parm status {
          description "boolean status parm";
          type boolean;
        }
      }
      output RpcDataReplyType;
    }
  }
}