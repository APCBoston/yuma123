<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns="http://netconfcentral.org/ncx/ifdata"
 targetNamespace="http://netconfcentral.org/ncx/ifdata"
 xmlns:xs="http://www.w3.org/2001/XMLSchema"
 elementFormDefault="qualified" attributeFormDefault="unqualified"
 xml:lang="en" version="1.0" xmlns:ncx="http://netconfcentral.org/ncx/1.0"
 xmlns:nc="urn:ietf:params:xml:ns:netconf:base:1.0"
 xmlns:n8="http://netconfcentral.org/ncx/types">
 <xs:annotation>
  <xs:documentation> Converted from NCX module by ncxdump v0.3

    Module: ifexamples
    Owner: ncx
